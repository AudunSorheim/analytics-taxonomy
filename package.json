{
  "name": "@navikt/analytics-taxonomy",
  "version": "0.6.0",
  "description": "Analytics taxonomy for NAV applications",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "npm run build:esm && npm run build:cjs && npm run build:cjs-package",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs-package": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "prepublishOnly": "npm test && npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/analytics-taxonomy.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/navikt/analytics-taxonomy/issues"
  },
  "homepage": "https://github.com/navikt/analytics-taxonomy#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "jest": "^29.7.0",
    "typescript": "^5.6.3"
  }
}
